{
  "name": "njuerp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:local": "vue-cli-service serve --mode local",
    "dev:remote": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@amcharts/amcharts4": "^4.10.9",
    "animejs": "^3.2.1",
    "core-js": "^3.6.5",
    "element-ui": "^2.13.2",
    "vue": "^2.6.12",
    "vue-particle-effect-buttons": "^1.0.4",
    "vue-router": "^3.4.6",
    "vuejs-progress-bar": "^1.2.2",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "node-sass": "^4.14.1",
    "sass-loader": "^10.0.4",
    "script-ext-html-webpack-plugin": "2.1.3",
    "style-loader": "^2.0.0",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "browser": true,
      "es6": true
    },
    "extends": [
      "plugin:vue/recommended",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "quotes": [
        2,
        "single",
        {
          "avoidEscape": true,
          "allowTemplateLiterals": true
        }
      ],
      "semi": [
        2,
        "never"
      ],
      "indent": [
        2,
        2,
        {
          "SwitchCase": 1
        }
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
